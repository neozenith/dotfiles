{
    "terminal.integrated.shell.windows": "%LOCALAPPDATA%\\Programs\\Git\\bin\\bash.exe",
    "terminal.integrated.fontFamily": "'SauceCodePro NF', Consolas, 'Courier New', monospace",   
    "terminal.integrated.fontWeight":"400",
    "editor.tabSize": 2,
    "editor.fontFamily": "'SauceCodePro NF', Consolas, 'Courier New', monospace",
    "explorer.confirmDelete": false,
    "editor.renderWhitespace": "all",
    "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
    "workbench.colorTheme": "Monokai",
    "editor.minimap.enabled": false,
    "git.confirmSync": false,
    "git.autofetch": true,
    "python.unitTest.pyTestEnabled": true,
    "python.unitTest.unittestEnabled": false,
    "python.unitTest.nosetestsEnabled": false,
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    "[python]": {
        "editor.rulers": [
            72,
            79
        ],
        "editor.tabSize": 4,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "vim.statusBarColorControl": true,
    "workbench.colorCustomizations": {
        "statusBar.background": "#624a37",
        "statusBar.noFolderBackground": "#624a37",
        "statusBar.debuggingBackground": "#624a37"
    },
    "vim.statusBarColors.visualline": "#624a37",
    "vim.statusBarColors.visualblock": "#624a37",
    "vim.statusBarColors.visual": "#624a37",
    "vim.statusBarColors.replace": "#623737",
    "vim.statusBarColors.normal": "#4a6237",
    "vim.statusBarColors.insert": "#375362",
    "vim.insertModeKeyBindings": [
        {
            "before": [
                ";",
                ";"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.foldfix": true,
    "vim.useCtrlKeys": false,
    "vim.easymotion": true,
}
